@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    ViewBag.Title = "DeleteUser";
}


<!DOCTYPE html>

<html>
<head>

    <meta name="viewport" content="width=device-width" />
    <link rel="stylesheet" href="~/Content/css/layout.css" type="text/css" media="screen" />

    <link href="~/Content/bootstrap.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/css/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/css/modern-business.css" rel="stylesheet" />
    <link href="~/Content/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link href="~/Content/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <script src="~/Content/js/bootstrap.min.js"></script>
    <script src="~/Content/js/bootstrap.js"></script>
    <meta name="viewport" content="width=device-width" />
    <title>Delete User</title>
    </head>


    <body>
        <header><h1 style="font-family:'Felix Titling'"> Delete Any User </h1></header>
        <div class="col-lg-9">
            <table class="table" style="border:solid; margin-top:20px;">
                <thead>
                    <tr class="btn-primary" style="text-align:center">
                        
                        <th>Name</th>
                        <th>Email</th>
                        <th>Password</th>
                        <th>Phone No</th>
                        <th>Delete</th>
                    </tr>
                </thead>

                @foreach (var obj in Model)
                {
                    if (!obj.Name.Equals("admin"))
                    {
                        <tr>
                            
                            <td>@obj.Name</td>
                            <td>@obj.Email</td>
                            <td>@obj.Password</td>
                            <td>@obj.PhoneNo</td>
                            <td><a href="/Admin/Delete2/@obj.Id">Delete</a></td>
                        </tr>
                    }
                }
            </table>
        </div>

</body>
</html>