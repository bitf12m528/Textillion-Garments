@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<!DOCTYPE html>

<html>
<head>

    <meta name="viewport" content="width=device-width" />
    <link rel="stylesheet" href="~/Content/css/layout.css" type="text/css" media="screen" />

    <link href="~/Content/bootstrap.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/css/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/css/modern-business.css" rel="stylesheet" />
    <link href="~/Content/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link href="~/Content/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <script src="~/Content/js/bootstrap.min.js"></script>
    <script src="~/Content/js/bootstrap.js"></script>
    <meta name="viewport" content="width=device-width" />
    <title>AddUser</title>
</head>
<body>
    <section id="main" style="height: 850px; background: url(/Content/images/FinBack.jpg) no-repeat center center fixed; -webkit-background-size: cover; -moz-background-size: cover; -o-background-size: cover; background-size: cover; ">
        <header><h1 style="font-family:'Felix Titling'"> Edit any User</h1></header>


        <div>
            <div class="col-lg-9">
                <table class="table" style="border:solid; margin-top:20px;">
                    <thead>
                        <tr class="btn-primary" style="text-align:center">
                            
                            <th>Name</th>
                            <th>PhoneNo</th>
                            <th>Email</th>
                            <th>Password</th>
                            <th>Edit</th>
                        </tr>
                    </thead>

                    @foreach (var obj in Model)
                    {
                        if (!obj.Name.Equals("admin"))
                        {
                            <tr>
                                
                                <td>@obj.Name</td>
                                <td>@obj.PhoneNo</td>
                                <td>@obj.Email</td>
                                <td>@obj.Password</td>
                                <td><a href="/Admin/Edit/@obj.Id">Edit</a></td>
                            </tr>
                        }
                    }
                </table>
            </div>
        </div>
        </section>
</body>
</html>