@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ViewAllUsers</title>
</head>
<body style="font-size:large">

    <header><h1 style="font-family:'Felix Titling'"> Delete All Users </h1></header>
    <div class="col-lg-9">
        <table class="table" style="border:solid; margin-top:20px;">
            <thead>
                <tr class="btn-primary" style="text-align:center">
                    <th>Id</th>
                    <th>Name</th>
                    <th>Password</th>
                    <th>Phone No</th>
                    <th>Email</th>

                </tr>
            </thead>

            @foreach (ProjectPhase1.Models.LogIn lg in Model)
            {
                <tr>
                    <td>@lg.Id"</td>
                    <td>@lg.Name</td>
                    <td>@lg.Password</td>
                    <td>@lg.PhoneNo</td>
                    <td>@lg.Email</td>

                </tr>
            }

        </table>
    </div>


</body>
</html>
