        @section IndexSec{
            <form class="navbar-form navbar-left pull-right" action="/Home/Search" role="search">
                <div class="form-group">
                    <input type="text" class="form-control" name="search" placeholder="search" />
                </div>
                <button type="submit" class="btn btn-success">Search</button>
            </form>
        }


        @foreach (var u in Model)
        {

            <div class="">

                <img src="~/Content/images/cart/@(u.Name).jpg" width="200" height="200" />

                <h4> Item Name : @u.Name </h4>

                <h4>Current Price : @u.Price</h4>

                @*<h4>Quantity : @u.qunatity</h4>*@

                @*<h4>Total : @u.Total</h4>*@

                <br />

            </div>
        }


